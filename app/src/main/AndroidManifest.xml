<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.windson">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StudentActivity"></activity>
        <activity android:name=".TeacherActivity"></activity>
        <activity android:name=".StudentSignUpActivity"></activity>
        <activity android:name=".TeacherSignUpActivity"></activity>
        <activity android:name=".StudentMyCoursesActivity"></activity>
        <activity android:name=".StudentMobileAppDevActivity"></activity>
        <activity android:name=".TeacherMyCoursesActivity"></activity>
        <activity android:name=".TeacherFeedbackActivity"></activity>
        <activity android:name=".TeacherMobileAppDevActivity"></activity>
    </application>

</manifest>